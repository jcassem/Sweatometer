{
  "Logging": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
  "AllowedHosts": "*",
  "MergeOptions": {
    "ReturnOnFirstResult": false,
    "CheckSynonyms": true,
    "CheckSynonymsOfParentWord": true,
    "MinimumMeanScoreForSynoymns": 65000,
    "MaxWordsToMeanLike": 20,
    "MaxWordsToSoundLike": 10,
    "MaxWordsToSpellLike": 10,
    "MergeResultsAreHyphenated": true
  }
}
